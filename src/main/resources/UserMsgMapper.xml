<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="usermsg.crud">
    <!--7、新增用户反馈-->
    <insert id="insertUserMasg" parameterType="usermsg">
        INSERT INTO useropinion (opId,opName,opEmail,opContent,opDate,opRemark)
        VALUES
        (#{opId},#{opName},#{opEmail},#{opContent},#{opDate},#{opRemark});
    </insert>
</mapper>